			/*进入样式*/
			
			.mui-bar {
				background-color: #43484e;
			}
			
			.mui-bar .mui-title,
			.mui-bar .mui-icon {
				color: #fff;
			}
			/*新闻滚动*/
			
			.root {
				height: 50px;
				line-height: 50px;
				border-top: 1px solid #ccc;
				font-size: 17px;
				overflow: hidden;
				background-color: #fff;
				position: relative;
			}
			
			.mainleft {
				float: left;
				padding: 0 10px;
				color: #0487d0;
				width: 35%;
				text-align: center;
				font-weight: bold;
			}
			
			.mainleft:after {
				content: '';
				height: 30px;
				top: 10px;
				width: 1px;
				position: absolute;
				left: 35%;
				background-color: #e6e6e6;
			}
			
			.mainright {
				float: right;
				padding: 0 15px;
				width: 65%;
			}
			
			@media screen and (max-width: 320px) {
				.mainright {
					font-size: 16px;
				}
			}
			/*常用应用*/
			
			.mui-col-sm-3 {
				width: 20%;
			}
			
			.mui-grid-view.mui-grid-9 {
				background-color: #fff;
				border: 0;
			}
			
			.mui-grid-view.mui-grid-9 .mui-table-view-cell {
				padding: 3px 0;
				border: 0;
			}
			
			#application {
				background-color: #fff;
				margin-top: 20px;
			}
			
			.addSubject {
				font-size: 15px;
			}
			
			.mui-bar-nav.mui-bar .mui-icon {
				line-height: 30px;
			}
			
			.title-select {
				display: inline-block;
				width: 25%;
				margin: 0 12px;
			}
			
			.title {
				height: 44px;
				background-color: #fff;
				text-align: center;
				line-height: 44px;
				font-size: 14px;
				color: #666;
			}
			
			.mui-bar-nav~.mui-content {
				padding-top: 49px;
			}
			
			.bg_sanjiao {
				background-position: 65px 6px;
				background-size: 14px 7px;
			}
			
			.hdead-img {
				border-radius: 20px;
				width: 42px !important;
				height: 42px !important;
			}
			
			.mui-card-header.mui-card-media .mui-media-body {
				line-height: 21px;
			}
			
			.mui-card {
				margin: 0px;
				margin-top: 15px;
				box-shadow: none;
				background: transparent;
			}
			
			.mui-card-content {
				padding: 10px;
				padding-bottom: 2px;
			}
			
			.username {
				color: #333;
			}
			
			.time {
				font-size: 12px;
				color: #999;
			}
			
			.left_ {
				margin-left: 10px;
			}
			
			.mui-card-header.mui-card-media .mui-media-body p {
				font-size: 12px;
			}
			
			.mui-card-footer:before,
			.mui-card-header:after {
				height: 0px;
			}
			
			.open_ {
				color: #0487d0;
				font-size: 16px;
			}
			
			.detail-img {
				width: 114px;
