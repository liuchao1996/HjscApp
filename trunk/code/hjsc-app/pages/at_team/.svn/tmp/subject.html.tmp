					setTimeout(function() {
						$("#topPopover").css('left', '0px');
					}, 20);
				});
			})

			$('#topPopover .mui-table-view-cell').on('tap', function() {
				var id = $(this).data('id');
				switch(id) {
					case 1:
						alert('删除');
						break;
					case 2:
						alert('收藏');
						break;
					case 3:
						alert('再议');
						break;
					case 4:
						alert('审核');
						break;
					case 5:
						alert('检查');
						break;
					case 6:
						alert('提交');
						break;
				}
				
				mui('#topPopover').popover('hide') 
