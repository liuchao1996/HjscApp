 * */

.mui-bar,
#cs_slider {
	height: 50px;
	background-color: #43484e;
}

.mui-bar .mui-title,
.mui-bar .mui-icon,
#cs_slider a {
	color: #fff;
}


/*整体容器*/

.mui-bar .mui-title {
	height: 50px;
	line-height: 50px;
}

.mui-table-view-cell:after {
	background-color: #E5E5E5;
}

.mui-bar-nav {
	box-shadow: none;
}

.bsnone {
	box-shadow: none;
}

.mui-table-view .mui-media-object {
	height: 60px;
	line-height: 60px;
	width: 60px;
	max-width: 60px
}


/*整体容器*/

.mui-table-view-cell {
	padding: 13px 15px;
}


/*右侧箭头*/

.mui-navigate-right:after,
.mui-push-right:after {
	right: 5px;
	content: '\e583';
	top: 30%;
	font-size: 30px;
	color: #CCCCCC;
	font-size: 26px;
}


/*消除上下点击1px*/

.mui-table-view-cell>a:not(.mui-btn) {
	position: relative;
	display: block;
	overflow: hidden;
	margin: -15px -15px;
	padding: inherit;
	text-overflow: ellipsis;
	color: inherit;
	white-space: inherit;
}

.list_title {
	color: #333;
	font-family: 'Helvetica';
	font-weight: bold;
}


/*开发版small样式*/

.ban {
	padding: 2px 4px;
	border-radius: 4px;
	font-size: 10px;
	vertical-align: middle;
	margin-left: 15px;
}

.zheng {
	border: 1px solid #02bf1b;
	color: #02bf1b;
}

.kai {
	border: 1px solid #00abbf;
	color: #00abbf;
}

.two_h {
	overflow: hidden;
	text-overflow: ellipsis;
	display: -webkit-box;
	-webkit-line-clamp: 2;
	-webkit-box-orient: vertical;
	word-wrap: break-word;
	word-break: break-all;
}


/*超出换行,注：让框架中含white-space属性需设为inherit;*/

.hbox {
	overflow: hidden;
	text-overflow: ellipsis;
	display: -webkit-box;
	-webkit-line-clamp: 3;
	-webkit-box-orient: vertical;
	width: 85%;
}


/*导航头部*/

.mui-bar .mui-icon:active {
	opacity: 1;
}


/*导航头，左边蓝色边框，右边字*/

.ntitle {
	width: 100%;
	height: 18px;
	line-height: 18px;
	font-size: 16px;
	color: #333;
	border-left: 3px solid #0487d0;
	margin: 16px 0 13px 18px;
	padding-left: 10px;
	font-weight: bold;
	display: inline-block;
	position: relative;
}


/*导航栏底部下划线*/

.ntitle:after {
	content: '';
	background-color: #f2f2f2;
	width: 91%;
	height: 1px;
	position: absolute;
	left: 0;
	bottom: -13px;
}